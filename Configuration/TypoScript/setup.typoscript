################################
#### KU SCROLL INDICATOR #######
################################
tt_content.ku_scroll_indicator =< lib.contentElement
tt_content.ku_scroll_indicator {
	templateName = ScrollIndicator
	templateRootPaths.10 = EXT:ku_scroll_indicator/Resources/Private/Templates/ContentElements/
	partialRootPaths {
		10 = EXT:ku_scroll_indicator/Resources/Private/Partials/ContentElements/
		# Fallback to templates from other extensions:
		20 = EXT:ku_prototype/Resources/Private/Partials/ContentElements/
		30 = EXT:bootstrap_package/Resources/Private/Partials/ContentElements/
		
	}

	dataProcessing {
		# Handle image upload:
		10 = TYPO3\CMS\Frontend\DataProcessing\FilesProcessor
		10 {
			references.fieldName = image
		}
	}
}
